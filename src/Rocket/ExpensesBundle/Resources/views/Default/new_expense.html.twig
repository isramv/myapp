{% extends 'RocketExpensesBundle:Default:base.html.twig' %}

{% block body %}
  <div class="container">
    <div class="row s12">
      {{ form_start(form) }}
      {{ form_widget(form) }}
      {{ form_end(form) }}
    </div>
  </div>
  {% if(asset('node_modules/materialize-css/dist/css/materialize.css')) %}
    <link rel="stylesheet" href="{{ asset('node_modules/materialize-css/dist/css/materialize.css') }}" />
    <script src="{{ asset('node_modules/materialize-css/dist/js/materialize.js') }}"> </script>
  {% endif %}
{% endblock %}
